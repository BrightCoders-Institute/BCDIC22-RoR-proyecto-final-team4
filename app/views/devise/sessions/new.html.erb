<div class="container_log_in">
  
    
    <%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
        <div class="form_users">
          <div class="logo">
            <%= image_tag "logo.png", class: "logo" %>
          </div>
          <div class="container_login">
            <h2>Log in</h2>
            <div class="field">
            <%= f.email_field :email, autofocus: true, autocomplete: "email", placeholder: "Email", class: "holder-login" %>
            </div>

            <div class="field">
              <%= f.password_field :password, autocomplete: "current-password", placeholder: "Password", class: "holder-password"%>
            </div>

            <div class="actions buttom">
              <%= f.submit "Log in" %>
            </div>
            
            <div class="links">
              <%= render "devise/shared/links"%>
            </div>
          </div>
          
        </div>
    <% end %>
        
</div>
